{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "no-console": "off",
    "import/no-named-as-default": "off"
  }
}
